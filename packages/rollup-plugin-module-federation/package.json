{
  "name": "rollup-plugin-module-federation",
  "version": "1.11.8",
  "description": "A rollup plugin for module federation",
  "keywords": [
    "rollup-plugin",
    "module-federation",
    "rollup",
    "plugin"
  ],
  "homepage": "https://github.com/MadaraUchiha-314/rollup-plugin-module-federation#readme",
  "bugs": {
    "url": "https://github.com/MadaraUchiha-314/rollup-plugin-module-federation/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadaraUchiha-314/rollup-plugin-module-federation.git"
  },
  "license": "MIT",
  "author": "Rohith R",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "scripts": {
    "build": "rm -rf dist/ && rollup --config rollup.config.mjs",
    "release": "semantic-release",
    "test:unit": "vitest run --coverage"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "@module-federation/runtime": "0.8.0",
    "@module-federation/sdk": "0.2.4",
    "estree-walker": "3.0.3",
    "magic-string": "0.30.1",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.5",
    "@semantic-release/git": "10.0.1",
    "@vitest/coverage-v8": "1.3.1",
    "rollup": "3.29.5",
    "rollup-plugin-copy": "3.4.0",
    "semantic-release": "21.0.7",
    "tslib": "2.6.2",
    "type-fest": "4.7.0",
    "typescript": "5.2.2",
    "vitest": "1.3.1",
    "@types/estree": "1.0.6"
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
